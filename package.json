{
  "name": "yunmicloud",
  "version": "3.0.0",
  "main": "./src/main.js",
  "private": true,
  "homepage": ".",
  "dependencies": {
    "urlencode": "^2.0.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@ant-design/icons": "^5.2.6",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "antd": "^5.13.2",
    "electron": "^36.5.0",
    "electron-builder": "^24.9.1",
    "html2canvas": "^1.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.3",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4",
    "webpack-cli": "^5.1.4"
  },
  "scripts": {
    "start": "set PORT=3001 && react-scripts start",
    "build": "react-scripts build",
    "build-preload": "webpack --config webpack.preload.config.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dist": "react-scripts build && webpack --config webpack.preload.config.js && electron-builder --win --x64",
    "linux": "react-scripts build && webpack --config webpack.preload.config.js && electron-builder --linux --x64",
    "mac": "react-scripts build && webpack --config webpack.preload.config.js && electron-builder --mac --x64"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "productName": "云觅节点",
    "extends": null,
    "appId": "com.yunmi.cloud",
    "directories": {
      "buildResources": "build",
      "output": "dist"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "files": [
      "./build/**/*",
      "./src/main.js",
      "./src/run.js",
      "./src/bundle.js"
    ],
    "win": {
      "icon": "public/logo512.png",
      "artifactName": "yunmicloud-setup-${version}.${ext}"
    },
    "linux": {
      "icon": "public/logo512.png",
      "artifactName": "yunmicloud-${version}.${ext}"
    },
    "mac": {
      "icon": "public/logo512.png"
    }
  }
}